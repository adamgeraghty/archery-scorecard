CREATE TABLE Shoot_Dates (
    id INTEGER PRIMARY KEY,
    name TEXT NOT NULL,
    shoot_date INTEGER NOT NULL -- Unix timestamp
);

insertData:
INSERT OR REPLACE
INTO Shoot_Dates(
    id,
    name,
    shoot_date
) VALUES (?, ?, ?);

getData:
SELECT * FROM Shoot_Dates;

deleteData:
DELETE FROM Shoot_Dates;